<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
  </startup>
  <appSettings>
    <add key="OpcServerUrl" value="opcda://localhost/OPCServer.WinCC" />
    <add key="HttpPort" value="8080" />
    <add key="UpdateInterval" value="1000" />
    <add key="BatchSize" value="500" />
    <add key="MaxConnections" value="100" />
    <add key="EnableCompression" value="true" />
    <add key="LogFile" value="logs\opc_agent.log" />
    <add key="LogLevel" value="Info" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-13.0.0.0" newVersion="13.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
